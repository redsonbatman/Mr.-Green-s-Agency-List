        const agencies = rawData.map(a => ({
            'Agency Name': (a['Agency Name'] || '').trim(),
            'Website URL': (a['Website URL'] && a['Website URL'] !== 'Not found') ? a['Website URL'].trim() : '',
            'Profile URL': (a['Profile URL'] && a['Profile URL'] !== 'Not found') ? a['Profile URL'].trim() : ''
        }));

        let currentData = [...agencies];
        let sortCol = 'Agency Name';
        let sortAsc = true;

        const tableBody = document.getElementById('tableBody');
        const searchInput = document.getElementById('searchInput');
        const sortSelect = document.getElementById('sortSelect');

        const fallbackIcon = "data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 24 24' fill='%23515154'%3E%3Cpath d='M19 19V4h-4V3H5v16H3v2h18v-2h-2zm-6 0H7V5h6v14zm4 0h-2V6h2v13z'/%3E%3C/svg%3E";

        function renderTable(data) {
            tableBody.innerHTML = '';
            
            if (data.length === 0) {
                tableBody.innerHTML = `
                    <tr>
                        <td colspan="3">
                            <div class="no-results">
                                <svg fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.172 16.172a4 4 0 015.656 0M9 10h.01M15 10h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                                </svg>
                                <h2>No agencies found</h2>
                                <p>Try adjusting your search terms.</p>
                            </div>
                        </td>
                    </tr>`;
                return;
            }

            data.forEach(agency => {
                const tr = document.createElement('tr');
                
                // Name & Logo extraction
                const tdName = document.createElement('td');
                let domain = null;
                if (agency['Website URL']) {
                    try {
                        domain = new URL(agency['Website URL']).hostname;
                    } catch(e) {}
                }

                const imgSrc = domain 
                    ? `https://www.google.com/s2/favicons?domain=${domain}&sz=128` 
                    : fallbackIcon;

                tdName.innerHTML = `
                    <div class="agency-brand">
                        <img src="${imgSrc}" alt="" class="agency-logo" onerror="this.onerror=null; this.src='${fallbackIcon}';" />
                        <span class="agency-name">${agency['Agency Name'] || 'Unknown'}</span>
                    </div>
                `;
                
                // Website
                const tdWeb = document.createElement('td');
                if (agency['Website URL']) {
                    tdWeb.innerHTML = `<a href="${agency['Website URL']}" target="_blank" class="link-badge btn-web">Visit Website</a>`;
                } else {
                    tdWeb.innerHTML = '<span class="no-link">—</span>';
                }

                // Profile
                const tdProf = document.createElement('td');
                if (agency['Profile URL']) {
                    tdProf.innerHTML = `<a href="${agency['Profile URL']}" target="_blank" class="link-badge btn-profile">View Profile</a>`;
                } else {
                    tdProf.innerHTML = '<span class="no-link">—</span>';
                }

                tr.appendChild(tdName);
                tr.appendChild(tdWeb);
                tr.appendChild(tdProf);
                tableBody.appendChild(tr);
            });
        }

        function applySort() {
            // Update Table Headers UI
            document.querySelectorAll('th').forEach(th => th.classList.remove('active'));
            document.querySelectorAll('.sort-icon').forEach(icon => {
                icon.innerHTML = '<span>▲</span><span>▼</span>';
            });

            const icon = document.getElementById(`sort-${sortCol}`);
            if (icon) {
                icon.parentElement.parentElement.classList.add('active');
                icon.innerHTML = sortAsc ? '<span>▲</span>' : '<span>▼</span>';
            }

            // Sync Dropdown
            let selectVal = "";
            if (sortCol === 'Agency Name') selectVal = sortAsc ? 'name_asc' : 'name_desc';
            if (sortCol === 'Website URL') selectVal = 'web_asc';
            if (sortCol === 'Profile URL') selectVal = 'prof_asc';
            sortSelect.value = selectVal;

            // Sort logic
            currentData.sort((a, b) => {
                let valA = a[sortCol].toLowerCase();
                let valB = b[sortCol].toLowerCase();
                
                if (valA < valB) return sortAsc ? -1 : 1;
                if (valA > valB) return sortAsc ? 1 : -1;
                return 0;
            });

            renderTable(currentData);
        }

        // Clicking a table header (Desktop)
        function handleHeaderSort(column, shortName) {
            if (sortCol === column) {
                sortAsc = !sortAsc;
            } else {
                sortCol = column;
                sortAsc = true;
            }
            applySort();
        }

        // Changing the select dropdown (Mobile/Alternative)
        sortSelect.addEventListener('change', (e) => {
            const val = e.target.value;
            if (val === 'name_asc') { sortCol = 'Agency Name'; sortAsc = true; }
            else if (val === 'name_desc') { sortCol = 'Agency Name'; sortAsc = false; }
            else if (val === 'web_asc') { sortCol = 'Website URL'; sortAsc = true; }
            else if (val === 'prof_asc') { sortCol = 'Profile URL'; sortAsc = true; }
            applySort();
        });

        // Search
        searchInput.addEventListener('input', (e) => {
            const term = e.target.value.toLowerCase();
            currentData = agencies.filter(a => a['Agency Name'].toLowerCase().includes(term));
            applySort(); // Re-sort and render
        });

        // INITIALIZE ON LOAD
        // Start sorted by name A-Z by default
        sortCol = 'Agency Name';
        sortAsc = true;
        applySort();

    </script>
</body>
</html>
